PROGBASE		    .equ 0x4000
PROGLOAD		    .equ 0x4000

NBUFS			    .equ 5

BANK_BITS		    .equ 0x18		; Spectrum ROM select
						; Video high

CONFIG_SWAP		    .equ 1

Z80_MMU_HOOKS		    .equ 0
